    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int x = Integer.parseInt(jTextField1.getText());
        int y = Integer.parseInt(jTextField2.getText());
        
        int sum;
        sum = x + y;
        int prod;
        prod = x * y;
        int diff;
        diff = x - y;     
        int qout;
        qout = x / y;
        
        jLabel1.setText(" the sum of 2 number is : " + String.valueOf(sum));
        jLabel2.setText(" the sum of 2 number is : " + String.valueOf(prod));
        jLabel3.setText(" the sum of 2 number is : " + String.valueOf(diff));
        jLabel4.setText(" the sum of 2 number is : " + String.valueOf(qout));
        
        
        JOptionPane.showMessageDialog(this, "computed successfully");
    }                                        

